  "description": "A straightforward method of managing state, with support for Promises, Generators, and Async/Await.",
  "main": "build/bundle.js",
  "scripts": {
    "rmrf": "rm -rf ./node_modules",
    "clean": "rm -rf ./build",
    "setup": "npm cache clear && npm run rmrf && npm install",
