{
  "name": "state-manager",
  "version": "0.0.1",
  "description": "A straightforward method of managing state, with support for Promises, Generators, and Async/Await.",
  "main": "build/bundle.js",
  "scripts": {
    "rmrf": "rm -rf ./node_modules",
    "clean": "rm -rf ./build",
    "setup": "npm cache clear && npm run rmrf && npm install",
    "setup:dev": "npm run setup && npm run build:dev",
    "start": "node server.prod.js",
    "start:dev": "node server.dev.js",
    "build": "npm run clean && webpack --config webpack.config.js --display-error-details",
    "postinstall": "npm run clean && npm run build",
    "build:dev": "npm run clean && webpack --config webpack.config.dev.js --display-error-details"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brochington/state-manager.git"
  },
  "author": "Broch Stilley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brochington/state-manager/issues"
  },
  "homepage": "https://github.com/brochington/state-manager#readme",
  "dependencies": {
    "babel-runtime": "^6.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.10.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.35.0",
    "webpack": "^1.13.0"
  }
}
